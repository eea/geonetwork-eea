  <skos:Concept xmlns:dc="http://purl.org/dc/terms/" rdf:about="http://rod.eionet.europa.eu/obligations/619">
    <skos:prefLabel xml:lang="en">Country fact sheets on waste policies</skos:prefLabel>
    <skos:definition xml:lang="en">Clarification on the reporting exercise will be provided by EEA to countries before the reporting deadline. Letter sent to Eionet on 7 April 2011 (password required) at: http://www.eionet.europa.eu/rolemails/roles-mail-2011/message_html?skey=date&amp;rkey=1&amp;id=99</skos:definition>
  </skos:Concept>
  <skos:Concept xmlns:dc="http://purl.org/dc/terms/" rdf:about="http://rod.eionet.europa.eu/obligations/630">
