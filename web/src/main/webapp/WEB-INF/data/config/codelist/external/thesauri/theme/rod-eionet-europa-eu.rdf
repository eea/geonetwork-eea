    <skos:prefLabel xml:lang="en">European Red list of Birds</skos:prefLabel>
    <skos:definition xml:lang="en"/>
  </skos:Concept>
  <skos:Concept xmlns:dc="http://purl.org/dc/terms/" rdf:about="http://rod.eionet.europa.eu/obligations/691">
